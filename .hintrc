{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "order",
          "object-fit",
          "flex-direction",
          "display: inline-grid",
          "scroll-behavior",
          "width: max-content",
          "filter",
          "grid-template-columns",
          "display: grid",
          "grid-template-columns: repeat(2, 1fr)",
          "grid-template-columns: repeat(6, 1fr)",
          "grid-template-columns: repeat(12, 1fr)",
          "grid-template-columns: subgrid",
          "grid-column"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off"
      }
    ],
    "button-type": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ]
  }
}